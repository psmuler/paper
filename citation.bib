  doi = {10.1016/j.jfoodeng.2019.01.015},
  abstract = {This work aimed at developing numerical models to predict the moisture and size of slices of gels containing gelatinized or native cornstarch and calcium alginate, during convective drying. The coupling of mass transfer and shrinkage simulated the drying kinetics with evaluation of effective mass diffusivity. Drying of gels containing 34\% wb cornstarch (GC90 and RC90 samples) were well fitted by the analytical solution of Fick's second law, with R2 of 0.963\textendash 0.998. The numerical model developed in finite element method via COMSOL Multiphysics better described the drying of gels formulated with 5.4\% wb of cornstarch (GC50 and RC50 samples), yielding R2 of 0.958\textendash 0.992. Shrinkage was estimated by the molar flux of water and simulated by the Arbitrary Lagrangian-Eulerian (ALE) method. The shrinkage term modified the drying rate profiles and a pseudo-constant rate period was observed. The developed model can be applied in drying studies of gels or foodstuffs with high shrinkage ratio.},
  langid = {english},
  keywords = {Alginate,Cornstarch,Drying,drying shrinkage,Finite elements method,gel,Gelatinization,prediction,Shrinkage},
  file = {/Users/ti/Zotero/storage/Z5LTRC8S/Silva Júnior et al. - 2019 - Modeling of convective drying of cornstarch-algina.pdf}
}

@article{SMITH1995191,
  title = {Shrinkage during Drying of Silica Gel},
  author = {Smith, D.M. and Scherer, G.W. and Anderson, J.M.},
  year = {1995},
  journal = {Journal of Non-Crystalline Solids},
  volume = {188},
  number = {3},
  pages = {191--206},
  issn = {0022-3093},
  doi = {10.1016/0022-3093(95)00187-5},
  abstract = {A model is provided for predicting gel shrinkage during drying by combining the empirical observations that: (1) the bulk modulus of a gel increases with density, {$\rho$}, according to Kp = K0 ({$\rho\rho$}y)m (2.5 {$\leq$} m {$\leq$} 4); and (2) the variation in pore radius, r, is approximately proportional to pore volume (contrary to the dependence, r {$\alpha$} {$\rho-$}13, conventionally assumed). No allowance is made for viscoelastic relaxation, so the model applies only for drying from an inert solvent. The model may be used to guide processing efforts to yield either aerogel-like materials with high porosity or xerogels with high density and small pore size for adsorbents, membranes, etc. The extent of shrinkage is governed by two dimensionless parameters, P and m. The quantity P = As {$\gamma$} cos(\texttheta )m{$\rho$}yK0 (where As is specific surface area, {$\gamma$} is surface tension, and \texttheta{} is the contact angle) represents the relative magnitudes of capillary pressure and gel stiffness, and m describes the variation of stiffness with density. For P values less than 1, the shrinkage upon drying is less than 10\%, and is reversible. For shrinkages greater than {$\backsimeq$} 50\%, the density increase is irreversible, and is proportional to P1/(m - 1). Predicted shrinkage is compared with experimental results for silica gels dried from different surface tension pore fluids (2 \textexclamdown{} {$\gamma$} \textexclamdown{} 68 dyn/cm), initial wet gel density (0.06 \textexclamdown{} {$\rho$}0 \textexclamdown{} 0.15 g/cm3), and gel stiffness (0.3 \textexclamdown{} K0 \textexclamdown{} 1 MPa).},
  keywords = {drying shrinkage,gel,prediction}
}

