@article{venkateswararaoEffectMethyltrimethoxysilaneSynthesis1999,
  title = {Effect of Methyltrimethoxysilane as a Synthesis Component on the Hydrophobicity and Some Physical Properties of Silica Aerogels},
  author = {Venkateswara Rao, A. and Haranath, D.},
  year = {1999},
  month = sep,
  journal = {Microporous and Mesoporous Materials},
  volume = {30},
  number = {2},
  pages = {267--273},
  issn = {1387-1811},
  doi = {10.1016/S1387-1811(99)00037-2},
  abstract = {The effects of adding methyltrimethoxysilane (MTMS) to the synthesis formulation on the hydrophobicity and physical properties of silica aerogels are reported. The molar ratio of the methanol (MeOH) solvent, water (H2O), and the ammonia (NH4OH) catalyst to tetramethoxysilane (TMOS) precursor was fixed at 1TMOS:12MeOH:4H2O:3.6\texttimes 10-3NH4OH throughout the experiment and the MTMS/TMOS molar ratio M was varied from 0 to 1.55. After gelation, the alcogels were dried supercritically by high-temperature solvent extraction. The hydrophobicity of the resulting aerogels was tested by measuring the water uptake by the aerogel as a function of time, after putting them directly on the surface of water. It was found that for M{$<$}0.26 the aerogels were less hydrophobic but more transparent ({$>$}90\% in the visible range), whereas for M{$>$}1.03 the aerogels were more hydrophobic but semi-transparent to opaque. Aerogels that possessed good hydrophobicity and transparency ({$\sim$}85\% in the visible range) were obtained with an M{$\approx$}0.70. An increase in the MTMS content in the gels shifted the pore size distribution towards larger pore radii with a broad distribution. In order to determine the thermal stability of the hydrophobic nature of the aerogels, they were heat-treated in air in the temperature range between 25 and 350\textdegree C. It was found that below 280\textdegree C the aerogel samples showed hydrophobic properties, whereas above 280\textdegree C the hydrophobicity vanished. This is due to the disappearance of the CH3 groups in the aerogels. The aerogels were characterized by optical transmittance, pore size distribution, BET surface area and infrared spectroscopy measurements.},
  langid = {english},
  keywords = {Aerogels,hydrophobicity,Hydrophobicity,MTMS,MTMS/TMOS,Optical transmittance,pore distribution,Pore size distribution,Specific surface area},
  file = {/Users/ti/Zotero/storage/XRM48ZMD/Venkateswara Rao and Haranath - 1999 - Effect of methyltrimethoxysilane as a synthesis co.pdf;/Users/ti/Zotero/storage/XYFE8VMX/S1387181199000372.html}
}

